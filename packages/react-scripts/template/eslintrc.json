        ],
        "no-template-curly-in-string": [
            "error"
        ],
        "array-callback-return": [
            "error"
        ],
        "block-scoped-var": [
            "error"
        ],
        "class-methods-use-this": [
            "error"
        ],
        "complexity": [
            "error",
            10
        ],
        "consistent-return": [
            "error"
        ],
        "curly": [
            "error"
        ],
        "default-case": [
            "error"
        ],
        "dot-location": [
            "error"
        ],
        "dot-notation": [
            "error"
        ],
        "eqeqeq": [
            "error"
        ],
        "max-classes-per-file": [
            "error"
        ],
        "no-caller": [
            "error"
        ],
        "no-else-return": [
            "error"
        ],
        "no-empty-function": [
            "error",
            {
                "allow": [
                    "arrowFunctions"
